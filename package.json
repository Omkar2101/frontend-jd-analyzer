    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.7",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "jsdom": "^22.1.0",
    "msw": "^2.10.2",
